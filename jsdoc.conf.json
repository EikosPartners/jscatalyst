
{
    "plugins": ["./vuejsdocplugin.js"],
    "recurseDepth": 10,
    "source": {
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

{
  "name": "jscatalyst",
  "version": "1.2.33",
  "description": "Open source Javascript toolkit for building dashboards",
  "author": "Eikos Partners",
  "main": "dist/jscatalyst.min.js",
  "module": "src/index.js",
  "unpkg": "dist/jscatalyst.min.js",
  "repository": "https://github.com/EikosPartners/jscatalyst",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --mode=development",
    "start": "npm run dev",
    "test": "nyc jest",
    "build": "node build/build.js",
    "test_build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config  ./build/webpack.release.conf.js",
    "npm_build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config  ./build/webpack.prodRelease.conf.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run test_build && npm run npm_build"
  },
  "dependencies": {
    "ag-grid": "^16.0.1",
    "ag-grid-enterprise": "^16.0.1",
    "ag-grid-vue": "^16.0.0",
    "axios": "^0.18.0",
    "chroma-js": "^1.4.0",
    "color-diff": "^1.1.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "d3": "^5.7.0",
    "d3-horizon-chart": "0.0.6",
    "google-charts": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "mini-css-extract-plugin": "^0.4.3",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.0",
    "raf": "^3.4.0",
    "socket.io": "^2.1.1",
    "topojson-client": "^3.0.0",
    "vue": "^2.5.17",
    "vue-color": "^2.6.0",
    "vue-resize": "^0.4.3",
    "vue-server-renderer": "^2.5.17",
    "vue-socket.io": "^2.1.1",
    "vuetify": "^1.2.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mediaeventservices/jest-environment-jsdom-external-scripts": "^21.2.2",
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "chromedriver": "^2.42.0",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-canvas-mock": "0.0.4",
    "jest-junit": "^3.6.0",
    "jest-serializer-vue": "^0.3.0",
    "jsdoc": "^3.5.5",
    "json-server": "^0.12.1",
    "node-notifier": "^5.1.2",
    "nyc": "^11.9.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.17",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.14.0",
    "semver": "^5.5.1",
    "shelljs": "^0.7.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.8",
    "vue-jest": "^1.0.2",
    "vue-loader": "^15",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "testURL": "http://localhost:8080",
    "setupFiles": [
      "<rootDir>/test/unit/setup.js",
      "raf/polyfill"
    ],
    "testRegex": "test/unit/specs/*/.*\\.spec.(js)$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@@/(.*)$": "<rootDir>/node_modules/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ],
    "exclude": [
      "test",
      "build",
      "config",
      "dist"
    ]
  }
}

{
  "name": "jscatalyst",
  "version": "1.0.0",
  "description": "Open source Javascript toolkit for building dashboards",
  "author": "Eikos Partners",
  "main": "dist/jscatalyst.js",
  "module": "src/index.js",
  "unpkg": "dist/jscatalyst.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "nyc jest",
    "build": "node build/build.js",
    "test_build": "NODE_ENV=production webpack --progress --hide-modules --config  ./build/webpack.release.conf.js",
    "npm_build": "NODE_ENV=production webpack --progress --hide-modules --config  ./build/webpack.prodRelease.conf.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "ag-grid": "^16.0.1",
    "ag-grid-enterprise": "^16.0.1",
    "ag-grid-vue": "^16.0.0",
    "axios": "^0.18.0",
    "chart.js": "^2.7.1",
    "chartist": "^0.11.0",
    "chartkick": "^2.3.3",
    "compression": "^1.7.2",
    "d3": "^5.0.0-rc.3",
    "d3-horizon-chart": "0.0.6",
    "d3-sankey": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.0",
    "plotly": "^1.0.6",
    "plotly.js": "^1.33.1",
    "raf": "^3.4.0",
    "socket.io": "^2.1.0",
    "spdy": "^3.4.7",
    "topojson-client": "^3.0.0",
    "vue": "^2.5.2",
    "vue-chartist": "^2.1.2",
    "vue-chartjs": "^3.2.1",
    "vue-chartkick": "^0.2.1",
    "vue-resize": "^0.4.3",
    "vue-server-renderer": "^2.5.13",
    "vue-socket.io": "^2.1.1-a",
    "vuelidate": "^0.6.2",
    "vuetify": "^1.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mediaeventservices/jest-environment-jsdom-external-scripts": "^21.2.2",
    "@vue/test-utils": "^1.0.0-beta.13",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "chromedriver": "^2.37.0",
    "copy-webpack-plugin": "^4.4.1",
    "coveralls": "^3.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-canvas-mock": "0.0.4",
    "jest-junit": "^3.6.0",
    "jest-serializer-vue": "^0.3.0",
    "jsdoc": "^3.5.5",
    "json-server": "^0.12.1",
    "node-notifier": "^5.1.2",
    "nyc": "^11.7.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.11.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vue-jest": "^1.0.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "testURL": "http://localhost:8080",
    "setupFiles": [
      "<rootDir>/test/unit/setup.js",
      "raf/polyfill"
    ],
    "testRegex": "test/.*\\.spec.(js)$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@@/(.*)$": "<rootDir>/node_modules/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ],
    "exclude": [
      "test",
      "build",
      "config",
      "dist"
    ]
  }
}
